#include <stdio.h>

int main() {
    int n;
    printf("Введіть натуральне число n: ");
    scanf("%d", &n);
    
    if (n <= 1 || n >= 150) {
        printf("n повинно бути в межах від 2 до 149.\n");
        return 1;
    }

    int count = 0;
    int m = 1;

    while (m < n) {
        if ((n / m) == (n % m)) {
            count++;
        }
        m++;
    }

    printf("Кількість рівних дільників числа %d: %d\n", n, count);

    return 0;
}
